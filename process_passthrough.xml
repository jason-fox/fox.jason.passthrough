<project name="fox.jason.passthrough" default="passthrough.process">
	<!--
		Iterate across all files marked format="passthrough" and process them
	-->
	<target name="passthrough.process">
		<copy file="${passthrough.input}" tofile="${passthrough.output}" overwrite="true"/>
	</target>
</project>